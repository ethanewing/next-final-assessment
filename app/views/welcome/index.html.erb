<h1>Welcome!</h1>
<% if @current_user %>
  <div><%= "Welcome, #{@current_user.first_name} #{@current_user.last_name}!" %></div>
  <div><%= link_to "Sign out", logout_path %></div>
<% else %>
  <div><%= link_to "Sign Up", new_user_path(role: "regular") %></div>
  <div><%= link_to "Sign In", login_path %></div>
<% end %>
